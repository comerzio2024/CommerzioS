{
  "name": "commerzio-services",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite dev --port 5000",
    "dev": "dotenv -e .env -- cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint client/src server shared --ext .ts,.tsx",
    "lint:fix": "eslint client/src server shared --ext .ts,.tsx --fix",
    "db:push": "dotenv -e .env -- drizzle-kit push",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:reset": "dotenv -e .env -- cross-env NODE_ENV=development tsx scripts/reset-and-seed.ts"
  },
